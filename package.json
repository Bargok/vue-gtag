    "jest": "^23.1.0",
    "jest-vue-preprocessor": "^1.4.0",
    "semantic-release": "^7.0.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16",
    "vue-template-es2015-compiler": "1.6.x",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3"
  }
